<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.lvjp.association.mapper.VideoMapper">

    <select id="selectLatest" resultType="videoInfo">
        select video_id,video_title,video_path
        from video
        order by create_time desc limit #{count}
    </select>

    <select id="selectById" parameterType="Integer" resultType="video">
        select *
        from video
        where video_id = #{id}
    </select>
</mapper>